{
  "name": "@hawk-tracker/core",
  "version": "0.0.0",
  "private": false,
  "type": "module",
  "description": "Core module for the web SDK",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "lint": "eslint \"src/**/*.{ts,js}\"",
    "lint:fix": "eslint \"src/**/*.{ts,js}\" --fix",
    "test": "jest",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "sdk",
    "core"
  ],
  "author": "",
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node": "^24.2.1",
    "@workspace/build-config": "workspace:*",
    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "rollup": "^4.46.2"
  },
  "dependencies": {
    "@fingerprintjs/fingerprintjs": "^4.6.2",
    "@types/pako": "^2.0.3",
    "pako": "^2.1.0",
    "tslib": "^2.8.1"
  }
